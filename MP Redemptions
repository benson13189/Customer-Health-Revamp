select 
store_number,
sum(case when multipack_indicator = 'Y' then 1 else 0 end) as MP_Redeemed_Ct,
sum(case when multipack_indicator = 'N' then 1 else 0 end) as MP_Unredeemed_Ct,
cast(MP_Redeemed_Ct/(MP_Redeemed_Ct + MP_Unredeemed_Ct) * 100 as decimal(10,2)) as Percent_MP_Eligible_Redeemed

from "PROD"."RETAILER_PERFORMANCE"."ALTRIA_DASHBOARD_REPORT_RAW_DATA"
where
POSSIBLE_MULTIPACK_QUANTITIES is not null
group by store_number
